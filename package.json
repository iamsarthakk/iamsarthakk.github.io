{
  "name": "iamsarthakk.github.io",
  "version": "1.0.0",
  "description": "My Website",
  "main": "index.html",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin images/* --out-dir=dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamsarthakk/iamsarthakk.github.io.git"
  },
  "author": "Sarthak Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamsarthakk/iamsarthakk.github.io/issues"
  },
  "homepage": "https://github.com/iamsarthakk/iamsarthakk.github.io",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "gh-pages": "^3.1.0",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "node-sass": "^7.0.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.12.9"
  }
}
